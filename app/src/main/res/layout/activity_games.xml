<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Views.Games">
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/back"
            android:id="@+id/imageView"
            android:alpha=".4"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="320dp"
            android:layout_height="170dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="637dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp" android:layout_marginEnd="46dp" app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:text="Univerzijada 2020"
                android:textSize="36dp"
                android:textColor="@android:color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView11" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="320dp"
            android:layout_height="42dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="197dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="584dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp" android:layout_marginEnd="46dp" app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:text="Moje utakmice"
                android:textSize="28dp"
                android:textColor="@android:color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView9"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="9dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="9dp"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toEndOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="350dp"
            android:layout_height="119dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="273dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="444dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="30dp" android:layout_marginEnd="31dp" app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:text="Filter"
                android:textSize="18dp"
                android:textColor="@android:color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView10"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="41dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="41dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
                android:layout_marginEnd="294dp" app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="Datum"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView14"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="87dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="123dp" android:layout_marginEnd="185dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:hint="Od"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:ems="4"
                android:id="@+id/datefrom"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="35dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="89dp"
                android:layout_marginEnd="210dp" app:layout_constraintEnd_toEndOf="parent"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:hint="Do"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:ems="4"
                android:id="@+id/dateto"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="35dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="150dp"
                android:layout_marginEnd="149dp" app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="Vreme"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView16"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="87dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="256dp" android:layout_marginEnd="53dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="time"
                android:hint="Od"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:ems="4"
                android:id="@+id/timefrom"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="35dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="219dp"
                android:layout_marginEnd="80dp" app:layout_constraintEnd_toEndOf="parent"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="time"
                android:hint="Do"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:ems="4"
                android:id="@+id/timeto"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="35dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="52dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="283dp"
                android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="Hala"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView19"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="57dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="30dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="99dp" android:layout_marginEnd="222dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="Naziv"
                android:textSize="14dp"
                android:textColor="@android:color/black"
                android:ems="4"
                android:id="@+id/stadiumFil"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="93dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="7dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="89dp" android:layout_marginEnd="210dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <Button
                android:text="Primeni"
                android:textSize="14dp"
                android:background="@android:color/holo_green_light"
                android:layout_width="77dp"
                android:layout_height="30dp"
                android:id="@+id/filter"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="256dp"
                android:layout_marginEnd="17dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="82dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="7dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TableLayout
            android:id="@+id/gameTable"
            android:layout_width="320dp"
            android:layout_height="296dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="395dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="132dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="45dp" android:layout_marginEnd="46dp" app:layout_constraintEnd_toEndOf="parent"
    >


    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>